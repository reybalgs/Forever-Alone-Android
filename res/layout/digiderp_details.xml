<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
  <TextView
  	android:text="Course Code: DIGIDES"
  	android:id="@+id/coursecode_digiderp"
  	android:layout_width="wrap_content" 
  	android:layout_height="wrap_content"
  	android:layout_marginLeft="10dp"
  	android:gravity="center">
  </TextView>
  <TextView
  	android:text="Section: S17"
  	android:id="@+id/section_digiderp"
  	android:layout_width="wrap_content" 
  	android:layout_height="wrap_content"
  	android:layout_below="@+id/coursecode_digiderp"
  	android:layout_alignLeft="@+id/coursecode_digiderp">
  </TextView>
  <TextView
  	android:text="Days: Monday and Thursday"
  	android:id="@+id/days_digiderp"
  	android:layout_width="wrap_content" 
  	android:layout_height="wrap_content"
  	android:layout_below="@+id/section_digiderp"
  	android:layout_alignLeft="@+id/section_digiderp">
  </TextView>
  <TextView
  	android:text="Timeframe: 0940-1110"
  	android:id="@+id/time_digiderp"
  	android:layout_width="wrap_content" 
  	android:layout_height="wrap_content"
  	android:layout_below="@+id/days_digiderp"
  	android:layout_alignLeft="@+id/days_digiderp">
  </TextView>
  <TextView
  	android:text="Instructor: Sir Herp Derp"
  	android:id="@+id/instructor_digiderp"
  	android:layout_width="wrap_content" 
  	android:layout_height="wrap_content"
  	android:layout_below="@+id/time_digiderp"
  	android:layout_alignLeft="@+id/time_digiderp">
  </TextView>
  <TextView
  	android:text="Description: OMG THIS"
  	android:id="@+id/descriptionstart_digiderp"
  	android:layout_width="wrap_content" 
  	android:layout_height="wrap_content"
  	android:layout_below="@+id/instructor_digiderp"
  	android:layout_alignLeft="@+id/instructor_digiderp">
  </TextView>
  <TextView
  	android:text="COURSE IS SO HARD"
  	android:id="@+id/description_digiderp"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_below="@+id/descriptionstart_digiderp"
  	android:layout_marginRight="10dp"
  	android:layout_alignLeft="@+id/descriptionstart_digiderp">
  </TextView>
  <Button
     android:layout_height="wrap_content"
     android:layout_width="wrap_content"
     android:text="Okay."
     android:layout_centerHorizontal="true"
     android:layout_below="@+id/description_digiderp"
     android:onClick="kaBoom"/>
</RelativeLayout>